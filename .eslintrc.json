{
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
        "browser": true,
        "jasmine": true,
        "jquery": true
    },
    "extends": ["eslint:recommended", "plugin:jasmine/recommended"],
    "plugins": ["jasmine"],
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
